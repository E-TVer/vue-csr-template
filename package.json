    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^4.34.0",
    "webpack-cli": "^3.3.4",
    "webpack-dev-server": "^3.7.1",
    "webpack-merge": "^4.2.1"
  },
  "dependencies": {
    "@babel/polyfill": "^7.4.4",
    "axios": "^0.19.0",
    "core-js": "^3.1.4",
    "vue": "^2.6.10",
    "vue-meta": "^2.0.3",
    "vue-router": "^3.0.6",
    "vuex": "^3.1.1",
    "vuex-router-sync": "^5.0.0"
