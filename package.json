{
  "name": "vue-spa-template",
  "version": "1.0.0",
  "description": "A vue-spa based scaffolding.",
  "main": "index.js",
  "scripts": {
    "build:prod": "cross-env NODE_ENV=production webpack --config build-config/webpack.config.prod.js --progress --hide-modules",
    "clean": "rimraf public",
    "build": "npm run clean && npm run build:prod",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --config build-config/webpack.config.dev.js",
    "lint": "eslint --ext .js --ext .jsx --ext .vue src/",
    "lint-fix": "eslint --fix --ext .js --ext .jsx --ext .vue src/"
  },
  "browserslist": [
    "> 0.25%",
    "last 2 versions",
    "not ie <= 9"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lyswhut/vue-spa-template.git"
  },
  "keywords": [
    "vue"
  ],
  "author": "lyswhut",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lyswhut/vue-spa-template/issues"
  },
  "homepage": "https://github.com/lyswhut/vue-spa-template#readme",
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.4.5",
    "autoprefixer": "^9.6.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.6",
    "cross-env": "^5.2.0",
    "css-loader": "^3.0.0",
    "eslint": "^5.16.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-formatter-friendly": "^6.0.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-html": "^5.0.5",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-standard": "^4.0.0",
    "file-loader": "^4.0.0",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "html-webpack-plugin": "^4.0.0-beta.5",
    "less": "^3.9.0",
    "less-loader": "^5.0.0",
    "mini-css-extract-plugin": "^0.7.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-loader": "^3.0.0",
    "pug": "^2.0.3",
    "pug-plain-loader": "^1.0.0",
    "rimraf": "^2.6.3",
